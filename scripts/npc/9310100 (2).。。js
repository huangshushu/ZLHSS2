                add += "#L13#情侣套装#l ";

                add += "#L12##r限时销售#l ";

                add += "#L11##b我是土豪#l ";

                add += "#L14#手套鞋子#l \r\n";

                cm.sendSimple(add);

            }

        //////////////////////////////////////////////////////////////////////////////////////////////////////

